<h1>stuff searched</h1>
<% @articles.each do |article| %>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h3 class="panel-title"><%= article.titre %>&nbsp &nbsp &nbsp<%= article.category %>&nbsp &nbsp &nbsp<%= article.adresse %>&nbsp &nbsp &nbsp<%= article.prix %></h3>
    </div>
    <div class="panel-body">
      <%= article.description %>
      <br>
      <%= link_to "see more", article %>
    </div>
  </div>
<% end %>